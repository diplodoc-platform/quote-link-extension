{
  "name": "@diplodoc/quote-link-extension",
  "version": "0.1.4",
  "description": "Quote link extension for Diplodoc platform",
  "main": "build/plugin/index.js",
  "types": "build/plugin/index.d.ts",
  "engines": {
    "npm": ">=11.5.1"
  },
  "exports": {
    ".": {
      "types": "./build/plugin/index.d.ts",
      "default": "./build/plugin/index.js"
    },
    "./runtime": {
      "types": "./build/runtime/index.d.ts",
      "style": "./build/runtime/index.css",
      "default": "./build/runtime/index.js"
    },
    "./runtime/styles": "./build/runtime/index.css",
    "./runtime/styles.css": "./build/runtime/index.css"
  },
  "homepage": "https://diplodoc.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diplodoc-platform/quote-link-extension.git"
  },
  "bugs": {
    "url": "https://github.com/diplodoc-platform/diplodoc/issues"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run build:clean && run-p build:*",
    "build:clean": "node scripts/clean.mjs",
    "build:js": "node esbuild/build.mjs",
    "build:declarations": "tsc -p tsconfig.publish.json --emitDeclarationOnly --outDir ./build",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest --config vitest.config.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "lint": "lint update && lint",
    "lint:fix": "lint update && lint fix",
    "pre-commit": "lint update && lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@diplodoc/utils": "^2.1.0"
  },
  "devDependencies": {
    "@diplodoc/lint": "^1.12.0",
    "@diplodoc/transform": "^4.62.0",
    "@diplodoc/tsconfig": "^1.0.2",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^22.19.7",
    "esbuild-sass-plugin": "^3.3.1",
    "markdown-it": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
